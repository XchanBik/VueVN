{
  "name": "vuevn",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:electron": "cd packages/editor && npm run dev:electron",
    "dev:editor": "cd packages/editor && npm run dev:editor",
    "build:editor": "cd packages/editor && npm run build",
    "build:engine": "cd packages/engine && npm run build",
    "build:runtime": "cd packages/runtime && npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.5",
    "typescript": "^5.3.3"
  }
}
